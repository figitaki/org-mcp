{
  "name": "org-mcp",
  "version": "0.1.0",
  "description": "MCP server for managing agentic coding workflows with Emacs org-mode",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node",
    "dev": "bun --watch src/index.ts",
    "start": "bun dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "bun test"
  },
  "keywords": [
    "mcp",
    "emacs",
    "org-mode",
    "agent",
    "workflow"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.24.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2"
  }
}
